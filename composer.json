{
    "name": "quetzal/settings-manager",
    "description": "Utility manager for loading different settings files with different formats",
    "license": "MIT",
    "authors": [
        {
            "name": "VoodooPrograms"
        }
    ],
    "autoload": {
        "psr-4": {
            "Quetzal\\SettingsManager\\": "src/"
        }
    },
    "autoload-dev": {
        "Quetzal\\SettingsManager\\Tests\\": "tests/"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "require": {
        "php": "^7.0",
        "subjective-php/spl-types": "^3.0",
        "symfony/yaml": "^5.0",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
